# As for advice on how to change settings please see
# http://dev.mysql.com/doc/refman/5.6/en/server-configuration-defaults.html

[mysqld]
datadir = /var/lib/mysql
socket  = /var/lib/mysql/mysql.sock
tmpdir = /var/tmp/mysql

port = 3306
back_log = 3000
character_set_server = utf8
max_connect_errors = 100
max_connections = 650
max_user_connections = 650
max_heap_table_size = 64M
max_allowed_packet = 1024M
max_binlog_size = 500M
thread_stack = 256K
interactive_timeout = 7200
wait_timeout = 86400
sort_buffer_size = 704KB
read_buffer_size = 704KB
read_rnd_buffer_size = 352KB
join_buffer_size = 352KB
net_buffer_length = 16K
thread_cache_size = 100
ft_min_word_len = 4
transaction_isolation = READ-COMMITTED
tmp_table_size = 2097152
table_open_cache = 2000
skip_name_resolve
core-file
lower_case_table_names = 1
log_bin_trust_function_creators = 1
log-bin = /var/lib/mysql/mysql-bin.log
log-bin-index = /var/lib/mysql/master-log-bin.index
log-error = /var/log/mysql/alert.log
relay-log = /var/log/mysql/slave-relay.log
relay-log-info-file = /var/lib/mysql/slave-relay-log.info
relay-log-index = /var/lib/mysql/slave-relay-log.index
master-info-file = /var/lib/mysql/master.info
log-slave-updates = 1
binlog_cache_size = 128KB
sync_binlog = 1000
log_warnings
slow_query_log_file = /var/log/mysql/slow_query.log
slow_query_log = 1
log_output = TABLE
long_query_time = 1
binlog_format = ROW
server_id = 780561050
auto_increment_increment = 1
auto_increment_offset = 1
slave_net_timeout = 60
key_buffer_size = 16M
bulk_insert_buffer_size = 4M
myisam_sort_buffer_size = 262144
myisam_max_sort_file_size = 2048K
myisam_repair_threads = 1
myisam_recover_options = FORCE
innodb_data_home_dir = /var/lib/mysql/
innodb_log_group_home_dir = /var/lib/mysql/
innodb_additional_mem_pool_size = 2097152
innodb_buffer_pool_size = 1229M
innodb_data_file_path = ibdata1:200M:autoextend
innodb_file_per_table=1
innodb_file_io_threads = 4
innodb_flush_log_at_trx_commit = 2
innodb_log_buffer_size = 8M
innodb_log_file_size = 1000M
innodb_log_files_in_group = 2
innodb_max_dirty_pages_pct = 90
innodb_flush_method = O_DIRECT
innodb_lock_wait_timeout = 50
innodb_doublewrite = 1
innodb_rollback_on_timeout = OFF
innodb_autoinc_lock_mode = 1
innodb_read_io_threads = 4
innodb_write_io_threads = 4
innodb_io_capacity = 2000
innodb_purge_threads = 1
master_info_repository = TABLE
relay_log_info_repository = TABLE
query_cache_type = 0
concurrent_insert = 1
query_cache_limit = 1048576
query_cache_min_res_unit = 1K
log-slow-admin-statements
innodb_stats_on_metadata = OFF
innodb_file_format = Barracuda
innodb_read_ahead = 0
innodb_thread_concurrency = 0
innodb_sync_spin_loops=20
innodb_spin_wait_delay=6
default_storage_engine = InnoDB
innodb_stats_sample_pages = 8
open_files_limit = 65535
gtid_mode = ON
enforce-gtid-consistency = 1
loose_performance_schema = off
loose_binlog_order_commits = OFF
innodb_ft_max_token_size = 84
log_bin_use_v1_row_events = 1
delay_key_write = ON
key_cache_division_limit = 100
innodb_old_blocks_pct = 37
loose_rds_gtid_precommit = ON
ft_query_expansion_limit = 20
loose_thread_pool_stall_limit = 30
loose_innodb_log_compressed_pages = OFF
init_connect = ''
innodb_print_all_deadlocks = OFF
delayed_insert_timeout = 300
loose_thread_pool_oversubscribe = 10
binlog_stmt_cache_size = 32768
net_retry_count = 10
binlog_checksum = CRC32
low_priority_updates = 0
autocommit = 1
key_cache_age_threshold = 300
innodb_concurrency_tickets = 5000
table_definition_cache = 512
innodb_use_native_aio = 1  
net_write_timeout = 60
loose_rds_threads_running_high_watermark = 50000
innodb_table_locks = ON
query_alloc_block_size = 8192
max_prepared_stmt_count = 16382
innodb_thread_sleep_delay = 10000
net_read_timeout = 30
loose_innodb_rds_min_concurrency_tickets = 50
loose_rds_ic_reduce_hint_enable = OFF
max_write_lock_count = 102400
innodb_old_blocks_time = 1000
innodb_stats_method = nulls_equal
max_length_for_sort_data = 1024
query_prealloc_size = 8192
innodb_large_prefix = OFF
delayed_insert_limit = 100
group_concat_max_len = 1024
innodb_disable_sort_file_cache = ON
innodb_ft_min_token_size = 3
loose_skip_symbolic_links = ON
key_cache_block_size = 1024
slow_launch_time = 2
loose_thread_handling = "one-thread-per-connection"
innodb_online_alter_log_max_size = 134217728
innodb_open_files = 3000
eq_range_index_dive_limit = 10
loose_innodb_adaptive_hash_index_parts = 8
div_precision_increment = 4
binlog_row_image = full
innodb_strict_mode = OFF
delayed_queue_size = 1000
default_week_format = 0
log_queries_not_using_indexes = OFF
innodb_read_ahead_threshold = 56
loose_slave_parallel_workers = 8
connect_timeout = 10
default_time_zone = SYSTEM
sql_mode = ''
slave_exec_mode = strict
query_cache_size = 3145728
innodb_adaptive_hash_index = ON
performance_schema = ON
innodb_purge_batch_size = 300
loose_innodb_rds_adaptive_tickets_algo = ON
loose_rpl_semi_sync_slave_trace_level = 1
loose_rpl_semi_sync_master_timeout = 1000
loose_rpl_semi_sync_master_trace_level = 1
loose_rpl_semi_sync_slave_enabled = ON
loose_rpl_semi_sync_master_enabled = ON
loose_rpl_semi_sync_master_wait_no_slave = ON

[mysqldump]
quick
max_allowed_packet = 64M

[mysql]
no-auto-rehash
prompt = "\\u@\\h : \\d \\R:\\m:\\s> "

[myisamchk]
key_buffer = 512M
sort_buffer_size = 512M
read_buffer = 8M
write_buffer = 8M

[mysqlhotcopy]
interactive-timeout

[mysqld_safe]
user = mysql
basedir = /var/lib/mysql
pid-file=/var/run/mysqld/mysqld.pid

[mysql_install_db]
basedir = /var/lib/mysql


